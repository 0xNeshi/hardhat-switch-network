{
    "compilerOptions": {
        "target": "ES2021",
        "module": "commonjs",
        "declaration": true,
        "sourceMap": true,
        "outDir": "./dist",
        "strict": true,
        "rootDir": "./src",
        "esModuleInterop": true,
        "noImplicitOverride": true,
        "skipLibCheck": true
    },
    "exclude": ["./dist", "node_modules"],
    "include": ["./src"]
}
